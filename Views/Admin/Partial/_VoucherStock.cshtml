
@model IEnumerable<RoundpayFinTech.AppCode.Model.VStock>
@if (Model != null && Model.Any())
{

    int i = 0;
    @foreach (var item in Model)
    {
        i++;

        <tr>
            <td>@i</td>
            <td>@item.ID</td>
            <td>@item.Amount</td>
            <td>@item.VoucherID</td>
            <td>@item.VoucherType</td>
            <td>@item.TotalCount</td>
            <td>@Convert.ToDecimal(@item.Amount * @item.TotalCount)</td>
            <td>@item.Remain</td>
            <td>@Convert.ToDecimal(@item.Amount * @item.Remain)</td>

        </tr>

    }

}

